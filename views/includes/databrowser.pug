#selectedData.card.mb-3
    .card-body.p-0
        table.table.table-sm.table-responsive.mb-0
            .noDataNotif No Data Selected
            tbody

h5 Browse

nav.nav.nav-tabs(role="tablist")
    a#nav-server-tab.nav-item.nav-link.active(data-toggle="tab" href="#nav-server" role="tab" aria-controls="nav-server" aria-selected="true")
        | Server
    a#nav-uploaded-tab.nav-item.nav-link(data-toggle="tab" href="#nav-uploaded" role="tab" aria-controls="nav-uploaded" aria-selected="false")
        | Uploaded
#dataBrowser.tab-content.p-2.bg-white.border.border-top-0
    #nav-server.tab-pane.show.active(role="tabpanel" aria-labelledby="nav-server-tab")
        #serverData.card.border-0
            .card-header#breadcrumbs.bg-white.small
            .card-body.p-0
                table.table.table-sm.table-hover.table-responsive.mb-0
                    tbody
            #overlay.position-absolute.w-100.h-100(style="display:none;background-color:rgba(255,255,255,0.5);")
    #nav-uploaded.tab-pane(role="tabpanel" aria-labelledby="nav-uploaded-tab")
        #uploadedData.card.border-0
            .card-header.bg-white
                form#uploadform.row.align-items-center
                    .col.px-0
                        input.w-100(name="uploadfile" type="file")
                    #filesizeNotif.small.mr-1 < 1.5 GB
                    button.btn.btn-primary(type="submit") Upload
            .card-body.p-0
                table.table.table-sm.table-hover.table-responsive.mb-0
                    tbody